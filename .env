# Neo ZMES - If using this file as a central file for ES, MLAPI and mariadb
ES_DEBUG_ENABLED=1
ES_COMMON_NAME=localhost
ES_ENABLE_AUTH=1

# mlapi container name or IP address
MLAPI_CONTAINER=mlapi
MLAPI_PORT=5000
# same as on the ES container, a default user. You can add to the db.
MLAPIDB_USER=mlapi_user
MLAPIDB_PASS=ZoneMinder
MLAPI_DEBUG_ENABLED=1
# Neo PYZM needs this
PYZM_CONF=/config



MYSQL_ROOT_PASSWORD=rootpassword
MYSQL_USER=mysqluser
MYSQL_PASSWORD=mysqlpassword


TZ=America/Chicago

MAX_LOG_SIZE_BYTES=1000000
MAX_LOG_NUMBER=20

EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_ADDRESS=coolemail@gmail.com
EMAIL_PASSWORD=very_secure_password


